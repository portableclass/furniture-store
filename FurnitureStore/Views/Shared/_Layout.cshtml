<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8"/>
	<meta name="viewport" content="width=device-width, initial-scale=1"/>
	<title>@ViewData["Title"]</title>
	<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css"/>
</head>
<body>
<div class="logo d-flex flex-wrap align-items-center justify-content-center border-bottom bg-primary py-3">
	<p class="text-white mb-0">TradeSup</p>
</div>
<div class="container">
	@{
		if (User.Identity.IsAuthenticated)
		{
			<partial name="_HeaderNav"/>
		}
	}
</div>
<noscript>You need to enable JavaScript to run this app.</noscript>
<div class="container">
	<main role="main" class="pb-3 border-bottom">
		@RenderBody()
	</main>
</div>
<div class="container">
	<footer class="py-3 my-4">
		@{
			if (User.Identity.IsAuthenticated)
			{
				<partial name="_FooterNav"/>
			}
		}
		<p class="text-center text-muted">Â© 2022 TradeSup, Inc</p>
	</footer>
</div>
<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
<script src="~/js/index.js"></script>
</body>
</html>
